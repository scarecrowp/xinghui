#import "XHUser.h"

@implementation XHUser
+ (XHUser *)sharedUser
{
    static XHUser *user;
    
    @synchronized(self)
    {
        if (!user)
            user = [[XHUser alloc] init];
        
        return user;
    }
}

@end
