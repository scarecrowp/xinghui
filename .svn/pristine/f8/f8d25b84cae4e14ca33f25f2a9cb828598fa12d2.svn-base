#import "ZXMySingleton.h"
 
@implementation ZXMySingleton
@synthesize goodArr;
+ (ZXMySingleton *)sharedSingleton
{
    static ZXMySingleton *sharedSingleton;
    
    @synchronized(self)
    {
        if (!sharedSingleton)
            sharedSingleton = [[ZXMySingleton alloc] init];
        
        return sharedSingleton;
    }
}

@end
